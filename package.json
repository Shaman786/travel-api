    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.45.1",
    "hono": "^4.11.4",
    "ioredis": "^5.9.2",
    "nanoid": "^5.1.6",
    "postgres": "^3.4.8",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@types/pg": "^8.16.0",
    "drizzle-kit": "^0.31.8",
